<html lang = 'en'>
    <title>
        <head>
            <h1>User Registration</h1>
        </head>
    </title>
    <body>
        <h2>User Registration</h2>
        <form>
            <br>
            <label for = 'username'>Username: </label>
            <input type="text" id = 'username' placeholder = 'username' name = 'username' required><br>
            <br>
            
            <label for = 'password'>Password:</label><br>
            <input type = "password" id = 'password' placeholder = 'password' name = "password" required><br>
            <br>
    
            <label for = 'name'>Name:</label><br>
            <input type = "text" id = 'name' placeholder = 'fullname' name = "fullname" required><br>
           
            Gender:<br>
            <input type = "radio" name = "gender" value = "male">Male<br>
            <input type = "radio" name = "gender" value = "male">Female<br>
            <br>

            <label for = 'email'>Email:</label><br>
            <input type = "email" id = 'email' placeholder = 'email_address' name = "email" required><br>
            
            <label for = 'phone'></label>Phone:<br>
            <input type = "number" id = 'phone' name = "phone"><br>

           

            <br>
            <input type = "submit" value="Register">
        </form>
    </body>
</html>
